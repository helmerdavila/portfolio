{
  "name": "helmer-portfolio",
  "private": true,
  "description": "My personal portfolio",
  "version": "1.0.0",
  "author": "Helmer Davila",
  "scripts": {
    "build": "NODE_OPTIONS=--max-old-space-size=8192 gatsby build --verbose",
    "clean": "gatsby clean",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "start": "gatsby clean && npm run develop",
    "lint": "eslint src/ --ext .tsx,.ts",
    "lint:fix": "eslint src/ --ext .tsx,.ts --fix",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:cov": "vitest run --coverage",
    "ts:check": "npx tsc --noEmit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{ts,tsx,jsx,css}": [
      "npm run lint:fix",
      "prettier --write"
    ],
    "**/*.mdx": "prettier --write"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.2.0",
    "@fortawesome/free-brands-svg-icons": "6.2.0",
    "@fortawesome/free-solid-svg-icons": "6.2.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@helmerdavila/fontawesomehelmer": "6.2.0",
    "@mdx-js/react": "2.3.0",
    "@sindresorhus/slugify": "^1.1.2",
    "classnames": "2.3.2",
    "cross-env": "7.0.3",
    "gatsby": "^5.7.0",
    "gatsby-omni-font-loader": "^2.0.2",
    "gatsby-plugin-canonical-urls": "5.7.0",
    "gatsby-plugin-client-side-redirect": "1.1.0",
    "gatsby-plugin-google-gtag": "5.7.0",
    "gatsby-plugin-image": "3.7.0",
    "gatsby-plugin-manifest": "5.7.0",
    "gatsby-plugin-mdx": "5.7.0",
    "gatsby-plugin-offline": "6.7.0",
    "gatsby-plugin-postcss": "6.7.0",
    "gatsby-plugin-react-helmet": "6.7.0",
    "gatsby-plugin-robots-txt": "1.8.0",
    "gatsby-plugin-sharp": "5.7.0",
    "gatsby-plugin-sitemap": "6.7.0",
    "gatsby-plugin-typescript": "5.7.0",
    "gatsby-remark-images": "^7.7.0",
    "gatsby-source-filesystem": "5.7.0",
    "gatsby-transformer-json": "5.7.0",
    "gatsby-transformer-sharp": "5.7.0",
    "lint-staged": "13.0.3",
    "prism-react-renderer": "1.3.5",
    "prism-themes": "1.9.0",
    "prismjs": "1.29.0",
    "prop-types": "15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-lottie-player": "^1.4.3",
    "react-scroll": "1.8.7",
    "react-slick": "0.29.0",
    "react-type-animation": "^2.1.1",
    "react-window-decorators": "1.0.7",
    "sass": "1.52.1",
    "slick-carousel": "1.8.1",
    "uuid": "9.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/classnames": "2.3.1",
    "@types/node": "18.14.0",
    "@types/react-helmet": "6.1.5",
    "@types/react-scroll": "1.8.3",
    "@types/react-slick": "0.23.10",
    "@types/react-typing-animation": "1.6.3",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.28.4",
    "autoprefixer": "10.4.12",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^21.1.0",
    "prettier": "2.8.1",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4",
    "vitest": "^0.28.4"
  }
}
